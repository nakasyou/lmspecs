{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "value": {
      "type": "object",
      "properties": {
        "overall": {
          "type": "number"
        }
      },
      "required": [
        "overall"
      ]
    },
    "references": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string"
          },
          "retrieved": {
            "type": "string"
          }
        },
        "required": [
          "url",
          "retrieved"
        ]
      }
    }
  },
  "required": [
    "value",
    "references"
  ]
}